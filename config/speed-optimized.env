# SPEED-OPTIMIZED CONFIGURATION
# This configuration file sets the fastest startup mode for the server

# Startup Settings
STARTUP_PRIORITY=quickstart
STARTUP_MODE=minimal

# Feature Flags (Disable non-essential features)
ENABLE_DATABASE_OPTIMIZATION=false
ENABLE_SECURITY_SCANS=false
ENABLE_BACKGROUND_TASKS=false
ENABLE_RATE_LIMITING=false
ENABLE_EXTRA_LOGGING=false
ENABLE_CONTENT_SCHEDULING=false

# Enable only essential features
ENABLE_WEBSOCKETS=true

# Compression settings (disable for faster startup)
ENABLE_COMPRESSION=false
CSRF_PROTECTION=false

# Database Settings
DB_POOL_MIN=1
DB_POOL_MAX=5
DB_IDLE_TIMEOUT=10000

# Security Settings (minimal for speed)
MAX_PAYLOAD_SIZE=10mb